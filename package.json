    "@types/classnames": "^2.3.1",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/jest": "^28.1.7",
    "@types/match-sorter": "^6.0.0",
    "@types/node": "^18.7.6",
    "@types/prettier": "^2.7.0",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
